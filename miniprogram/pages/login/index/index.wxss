/* 导航栏 */
.ellipse {
  width: 750rpx;
  height: 500rpx;
  margin-left: 0;
  margin-top: -150rpx;
  padding-top: 150rpx;
  border-radius: 250% / 50%;
}
/* 信息 */
.header .infor {
  height: 128rpx;
}

.header .avatar {
  margin-left: 275rpx;
  width: 200rpx;
  height: 200rpx;
  border-radius: 100%;
  border: 5rpx #66b5ff solid;
  box-shadow: 0rpx 0rpx 10rpx #66b5ff;
}

.header .infor .name {
  margin-top: 0rpx;
  margin-left: 295rpx;
  font-weight: 1000;
  font-size: 40rpx;
  color: white;
  z-index: 999;
}

.header .infor .detail {
  margin-top: 10rpx;
  display: inline-block;
  margin-left: 202rpx;
  font-size: 24rpx;
  z-index: 999;
  letter-spacing: 6rpx;
  font-weight: 500;
  color: #ebfffe;
}

.onloading {
  font-size: 64rpx;
  margin-left: 250rpx;
  margin-top: 200rpx;
}

.onloading .loadImage {
  display: inline;
}

.onloading .loadImage::after {
  margin-top: 13rpx;
  margin-right: 12rpx;
  width: 64rpx;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 64rpx;
}

#loginBtn{
  width: 400rpx;
  margin-left: 175rpx;
  margin-top: 200rpx;
}

#getIDERR{
  width: 600rpx;
  margin-left: 75rpx;
  margin-top: 200rpx;
}

#submit{
  width: 400rpx;
  margin-left: 175rpx;
  margin-top: 20rpx;
}

.sss{
  height: calc(100vh - 400rpx);
